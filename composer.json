{
  "name": "kennedytedesco/hashids-wrapper",
  "description": "Just a simple wrapper to Hashids.",
  "keywords": ["hashids", "wrapper"],
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Kennedy Tedesco",
      "email": "kennedyt.tw@gmail.com"
    }
  ],
  "require": {
    "php": "^7.2|^8.0",
    "hashids/hashids": "^4.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.4"
  },
  "autoload": {
    "psr-4": {
      "KennedyTedesco\\HashIdsWrapper\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests"
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true
  }
}
